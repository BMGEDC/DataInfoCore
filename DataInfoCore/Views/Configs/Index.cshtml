@model IEnumerable<DataInfoCore.Models.Config>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" >
    <thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Market.MarketName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Wholesaler.WholesalerName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Summary)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.DataPeriod)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Active)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Function)
			</th>


			<th>
				@Html.DisplayNameFor(model => model.ExternalProcess)
			</th>

			<th></th>
		</tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Market.MarketName)
            </td>
			
            <td>
                @Html.DisplayFor(modelItem => item.Wholesaler.WholesalerName)
            </td>
			<td>
				@Html.DisplayFor(modelItem => item.Summary)
			</td>
            <td>
                @Html.DisplayFor(modelItem => item.DataPeriod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Function)
            </td>
            
            
            <td>
                @Html.DisplayFor(modelItem => item.ExternalProcess)
            </td>
           
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
